module blinker (
    input clk,  // clock
    input rst,  // reset
    output out[3],
    output wrong
  ) {
  
   dff counter[28](.clk(clk), .rst(rst));

  always {
    wrong = counter.q[23];
    out[0] = counter.q[25];
    out[1] = counter.q[26];
    out[2] = counter.q[27];
    counter.d = counter.q + 1;
  }
}
